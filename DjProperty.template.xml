<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>C#</_Group>
      <Version />
      <MimeType>text/x-csharp</MimeType>
      <Shortcut>DjProperty</Shortcut>
      <_Description>Add a simple auto-property</_Description>
      <TemplateType>Expansion</TemplateType>
    </Header>
    <Variables>
      <Variable name="property-type">
        <Default>object</Default>
      </Variable>
      <Variable name="property-name">
        <Default>Name</Default>
      </Variable>
      <Variable name="private">
        <Default>private </Default>
      </Variable>
    </Variables>
    <Code><![CDATA[public $property-type$ $property-name$ { get; $private$set; }$end$]]></Code>
  </CodeTemplate>
</CodeTemplates>